	echo"
 					<script>
   						window.location='pantallaporfecha1.php';
    					alert('La Fecha DESDE es mayor que HASTA');
	 				</script>
 					";
	
	}else{
		
		echo"
 					<script>
   						window.location='salidasxfecha.php?desde=$desde&hasta=$hasta';
   	 				</script>
 					";
	}
?>